# RUN: llc -mtriple=x86_64-- -run-pass=shrink-wrap2 %s -enable-shrink-wrap2=true -debug-only=shrink-wrap2 -o /dev/null 2>&1 | FileCheck %s
---
name:            foo
tracksRegLiveness: true
body:             |
  bb.0:
    successors: %bb.1, %bb.2

    NOOP implicit-def %eflags
    JNE_1 %bb.2, implicit killed %eflags
    JMP_1 %bb.1

  bb.1:
    successors: %bb.3, %bb.4

    NOOP implicit-def %eflags
    JE_1 %bb.3, implicit killed %eflags
    JMP_1 %bb.4

  bb.2:
    successors: %bb.4

    %ebx = MOV32ri 12
    JMP_1 %bb.4

  bb.3:
    RET 0

  bb.4:
    %ebx = MOV32ri 12
    RET 0

...
#CHECK-LABEL: foo

#CHECK: BB#2 uses : %RBX
#CHECK-NEXT: BB#4 uses : %RBX
#CHECK: **** Shrink-wrapping results ****
#CHECK-NEXT: BB#2: Saves: %RBX, | Restores:
#CHECK-NEXT: BB#4: Saves: | Restores: %RBX,
#CHECK-NEXT: BB#5: Saves: %RBX, | Restores:
