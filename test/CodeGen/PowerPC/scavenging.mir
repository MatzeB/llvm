# RUN: llc -march=ppc64 -run-pass scavenger-test -verify-machineinstrs -o - %s | FileCheck %s
--- |
  define void @scav0() { ret void }
  define void @scav1() { ret void }
...
---
# CHECK-LABEL: name: scav0
name: scav0
tracksRegLiveness: true
registers:
  - { id: 0, class: g8rc }
stack:
  # variable-sized object should be a reason to reserve an emergency spillslot
  # in the RegScavenger
  - { id: 0, type: variable-sized, offset: -32, alignment: 1 }
body: |
  bb.0:
    %x0 = IMPLICIT_DEF
    %x1 = IMPLICIT_DEF
    %x2 = IMPLICIT_DEF
    %x3 = IMPLICIT_DEF
    %x4 = IMPLICIT_DEF
    %x5 = IMPLICIT_DEF
    %x6 = IMPLICIT_DEF
    %x7 = IMPLICIT_DEF
    %x8 = IMPLICIT_DEF
