# RUN: llc -mtriple=aarch64-none-linux-gnu -run-pass machine-scheduler  -verify-machineinstrs  -o - %s | FileCheck %s
---
# CHECK-LABEL: name: load_imp-def
# CHECK: bb.0.entry:
# CHECK: LDRWui %x0, 0
# CHECK: LDRWui %x0, 1
# CHECK: STRWui %w1, %x0, 2
name: load_imp-def
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: %w1, %x0
    %w8 = LDRWui %x0, 1, implicit-def %x8  :: (load 4)
    STRWui killed %w1, %x0, 2 :: (store 4)
    %w9 = LDRWui killed %x0, 0, implicit-def %x9  :: (load 4, align 8)
    %x0 = ADDXrr killed %x9, killed %x8
    RET_ReallyLR implicit %x0
...

